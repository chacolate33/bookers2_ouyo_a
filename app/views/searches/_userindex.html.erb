 <table class="table">
    <thead>
      <tr>
        <td>image</td>
        <td>name</td>
        <td colspan="4"></td>
      </tr>
    </thead>
    <tbody>
    <% users.each do |user| %>
     <tr>
      <td>
        <%= attachment_image_tag user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "40x40" %>
      </td>
      <td>
       <%= user.name %>
      </td>
      <td>
        <%= link_to 'Show', user_path(user.id) %>
      </td>
     </tr>
     <% end %>
    </tbody>
   </table>